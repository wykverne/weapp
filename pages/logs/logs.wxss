/**pages/logs/logs.wxss**/
.log-list {
  padding: 0 40rpx;
}

.log-header {
  padding: 40rpx 0;
  border-bottom: 2rpx solid #eee;
  margin-bottom: 20rpx;
}

.log-title {
  font-size: 36rpx;
  font-weight: bold;
  color: #333;
  display: block;
  margin-bottom: 10rpx;
}

.log-count {
  font-size: 24rpx;
  color: #999;
}

.log-item {
  margin: 20rpx 0;
  padding: 30rpx;
  background-color: #fff;
  border-radius: 12rpx;
  box-shadow: 0 2rpx 8rpx rgba(0, 0, 0, 0.1);
}

.log-time {
  font-size: 32rpx;
  color: #333;
  font-weight: 500;
  margin-bottom: 15rpx;
}

.log-detail {
  display: flex;
  align-items: center;
  font-size: 26rpx;
}

.log-label {
  color: #666;
  margin-right: 10rpx;
}

.log-value {
  color: #999;
  font-family: monospace;
}

.empty-state {
  text-align: center;
  padding: 100rpx 0;
}

.empty-text {
  font-size: 28rpx;
  color: #999;
}