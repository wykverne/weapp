<!--pages/logs/logs.wxml-->
<view class="container log-list">
  <view class="log-header">
    <text class="log-title">访问日志</text>
    <text class="log-count">共 {{logs.length}} 条记录</text>
  </view>
  
  <block wx:for="{{logs}}" wx:key="timeStamp">
    <view class="log-item">
      <view class="log-time">{{item.date}}</view>
      <view class="log-detail">
        <text class="log-label">时间戳:</text>
        <text class="log-value">{{item.timeStamp}}</text>
      </view>
    </view>
  </block>
  
  <view wx:if="{{logs.length === 0}}" class="empty-state">
    <text class="empty-text">暂无访问记录</text>
  </view>
</view>